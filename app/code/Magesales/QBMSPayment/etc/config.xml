<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <payment>
            <qbmspayment>
                <active>1</active>
                <model>QBMSPaymentFacadeAdapter</model>
                <title>Credit Card (Processed by QBMS)</title>
                <live_gateway_url>https://api.intuit.com/quickbooks/v4/payments/charges</live_gateway_url>
                <sandbox_gateway_url>https://sandbox.api.intuit.com/quickbooks/v4/payments/charges</sandbox_gateway_url>
                <is_connected>0</is_connected>
                <show_logo>0</show_logo>
                <ccfields>card_cvn,card_expiry_date,card_number</ccfields>
                <payment_action>authorize_capture</payment_action>
                <order_status>processing</order_status>
                <cctypes>AE,VI,MC,DI,JCB</cctypes>
                <allowspecific>0</allowspecific>
                <mode>0</mode>
                <can_initialize>0</can_initialize>
                <can_authorize>1</can_authorize>
                <can_capture>1</can_capture>
                <can_capture_partial>1</can_capture_partial>
                <can_void>1</can_void>
                <can_refund>1</can_refund>
                <debug>1</debug>
                <can_refund_partial_per_invoice>1</can_refund_partial_per_invoice>
                <can_use_internal>1</can_use_internal>
                <can_use_checkout>1</can_use_checkout>
                <is_gateway>1</is_gateway>
                <paymentInfoKeys>card_number,cc_type,card_expiry_date</paymentInfoKeys>
            </qbmspayment>
        </payment>
    </default>
</config>
