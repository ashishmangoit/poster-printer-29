<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd" layout="2columns-right">
    <head><title>Custom Quote</title></head>

    <body>
    	 <referenceContainer name="page.messages">
	        <block class="Magento\Framework\View\Element\Template" name="ajax.message.placeholder" template="Magento_Theme::html/messages.phtml"/>
	        <block class="Magento\Framework\View\Element\Messages" name="messages" as="messages" template="Magento_Theme::messages.phtml"/>
		</referenceContainer>
        <referenceContainer name="content">
            <block class="Mangoit\CustomQuote\Block\Quote" name="quote" template="quote.phtml">
            </block>
        </referenceContainer>
    </body>
</page>